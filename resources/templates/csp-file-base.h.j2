/**< user code begin header, do not change this comment! */
{{ CSP.user_code["header"] }}
/**> user code end header, do not change this comment! */
{% set header_guard = CSP.file.upper() | replace(".", "_") | replace(" ", "_") | replace("-", "_") %}
#ifndef CSP_{{ header_guard }}
#define CSP_{{ header_guard }}

#ifdef __cplusplus
extern "C" {
#endif /** __cplusplus */

/********************************** includes **********************************/
{%- block includes %}
{%- endblock %}
/**< user code begin includes, do not change this comment! */
{{ CSP.user_code["includes"] }}
/**> user code end includes, do not change this comment! */

/*********************************** typedef **********************************/
{%- block typedef %}
{%- endblock %}
/**< user code begin typedef, do not change this comment! */
{{ CSP.user_code["typedef"] }}
/**> user code end typedef, do not change this comment! */

/*********************************** define ***********************************/
{%- block define %}
{%- endblock %}
/**< user code begin define, do not change this comment! */
{{ CSP.user_code["define"] }}
/**> user code end define, do not change this comment! */

/*********************************** extern ***********************************/
{%- block extern %}
{%- endblock %}
/**< user code begin extern, do not change this comment! */
{{ CSP.user_code["extern"] }}
/**> user code end extern, do not change this comment! */

/********************************** functions *********************************/
{%- block functions_prototypes %}
{%- endblock %}
/**< user code begin functions prototypes, do not change this comment! */
{{ CSP.user_code["functions prototypes"] }}
/**> user code end functions prototypes, do not change this comment! */

/**< user code begin 0, do not change this comment! */
{{ CSP.user_code["0"] }}
/**> user code end 0, do not change this comment! */

#ifdef __cplusplus
} /* extern "C" */
#endif /** __cplusplus */

#endif /** CSP_{{ header_guard }} */
